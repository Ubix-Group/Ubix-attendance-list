<?xml version='1.0' encoding='utf-8'?>
<widget id="ishigo1987.ubixattendancelist" xmlns:cdv="http://cordova.apache.org/ns/1.0" xmlns="http://www.w3.org/ns/widgets" xmlns:android="http://schemas.android.com/apk/res/android" version="0.1.0" android-versionCode="100" >
  <name>ubix-attendance-list</name>
  <description>
    Ubix Attendance List
  </description>
  <author email="stephnguets@yahoo.fr">
    ishigo1987
  </author>
   
  <preference name="Orientation" value="portrait" />
  <preference name="KeepRunning" value="true"/>
  <preference name="AndroidExtraFilesystems" value="files,files-external,documents,sdcard,cache,cache-external,assets,root" />
  <plugin name="cordova-plugin-android-softinputmode" spec="https://github.com/ishigo1987/cordova-plugin-android-softinputmode.git" />
  <preference name="EnableDeveloperConsole" value="$IS_DEBUG" />
</widget>
